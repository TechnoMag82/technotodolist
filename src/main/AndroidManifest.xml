<?xml version="1.0" encoding="utf-8"?>
<manifest package="technomag.technotodolist"
    android:versionCode="7"
    android:versionName="2.4" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="25" />

    <application
        android:name="technomag.technotodolist.TechnoTodoListApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_cat_about"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:logo="@drawable/ic_cat_about">
        <activity
            android:name="technomag.technotodolist.MainActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
